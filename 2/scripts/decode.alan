# This is destination array of strings
set dst,     []
set decLen,  0
set srcLen,  0
set i,       0
set k,       0
set ch,      ''
# One string from array of strings
set keyword, ''
set prevCh,  ''
set n,       0

# Length of source array should be it's length - 1
len src,     srcLen
dec srcLen

:DECKEYWORDSTART
    cut    src, i, keyword

    set    k, 0
    set    prevCh, 119
    len    keyword, decLen
    dec    decLen
    set    decStr, ''
    :DECSTART
        cut    keyword, k, ch
        asc    ch, ch
        xor    ch, prevCh, n
        set    prevCh, ch
        char   n,  ch
        append decStr, ch

        inc    k
        gotog  k, decLen, DECEND
        goto   DECSTART
    :DECEND
    append dst, decStr

    inc    i
    gotog  i, srcLen, DECKEYWORDEND
    goto   DECKEYWORDSTART

:DECKEYWORDEND
echo dst